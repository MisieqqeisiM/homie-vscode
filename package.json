{
  "name": "homie",
  "displayName": "homie",
  "description": "Language support for homie",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "homie",
        "aliases": [
          "homie",
          "homie"
        ],
        "extensions": [
          ".hom"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/homie-icon.png",
          "dark": "./icons/homie-icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "homie",
        "scopeName": "source.homie",
        "path": "./syntaxes/homie.tmLanguage.json"
      }
    ]
  }
}